<script type="text/javascript">
    $('#service-slogan .img').click(function(e) {
    var index = $('#service-slogan .img').index(this);
    $('#service-slogan .arrow').addClass('hide');
    $('.slogan-content-block').addClass('hide');
    $('.slogan-content-block').eq(index).removeClass('hide');
    $('#service-slogan .arrow').eq(index).removeClass('hide');
   });
/*    $('.scroll-block-hover').mouseleave(function () {
        var index = $('.scroll-block').index(this);
        $('.scroll-block').eq(index).animate({
            height: '0px'
        })
    });*/
    $('.scroll-block').mouseover(function () {
        var index = $('.scroll-block').index(this);
        $('.scroll-block-hover').eq(index).animate({
            height: '200px'
        })
    });
    $('.scroll-block').mouseleave(function () {
        var index = $('.scroll-block').index(this);
        $('.scroll-block-hover').eq(index).animate({
            height: '0px'
        })
    });

</script>
